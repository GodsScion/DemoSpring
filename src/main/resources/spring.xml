
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<!-- #10 Constructor and Setter Injection  -->

    <bean id="devId" class="com.springbokka.DemoSpring.Dev">
        <!-- defaults to laptop -->
        <constructor-arg name="computer" ref="laptopId"/>
    </bean>
    
    <bean id="laptopId" class="com.springbokka.DemoSpring.Laptop">
    </bean>

    <bean id="desktopId" class="com.springbokka.DemoSpring.Desktop">
    </bean>

</beans>